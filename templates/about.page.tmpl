{{template "base" .}}
{{define "content"}}
<div class="container">
    <div class="row">
        <div class="col">
            <h1>This is About Page</h1>
            <p>
                {{if ne (index .StringMap "remote_ip") ""}}
                Remote IP: {{index .StringMap "remote_ip"}}
                {{else}}
                Not Remote IP
                {{end}}
            </p>
        </div>
    </div>
</div>
{{end}}